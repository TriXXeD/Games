					["custom"] = "local duration = 25\nlocal spellID = 18540\n--\n--\n--\naura_env.up = false\naura_env.cd = false\n--if OOMM_WA_BORDER then \n-- OOMM_WA_BORDER:BorderCreate(aura_env.id)\n--  OOMM_WA_BORDER:BorderSetPoint(aura_env.id)\n--end\naura_env.Update = function(endtime,p)\n    if tostring(endtime) ~= \"1.#INF\" then\n        local _,cooldown = GetSpellCooldown(spellID)\n        local starttime = WA_OOMM_WILFRED_DG or 0\n        local activeend = starttime + duration\n        local countdown = activeend - GetTime()\n        local downtime = cooldown - duration\n        if cooldown > 0 and countdown < 0 then -- spell on cd\n            aura_env.cd = true\n            aura_env.up = false\n            WeakAuras.regions[aura_env.id].region.icon:SetDesaturated(true)\n            --WeakAuras.regions[aura_env.id].region.icon:SetVertexColor(1,1,1)\n            WeakAuras.regions[aura_env.id].region.stacks:SetVertexColor(1,0,0,1) --cooldown color\n            --WeakAuras.regions[aura_env.id].region.icon:SetBlendMode(\"BLEND\")\n            \n            --if OOMM_WA_BORDER then \n            --  OOMM_WA_BORDER:BorderHide(aura_env.id)\n            --end\n            return p or \"\"\n        elseif cooldown > 0 and countdown >= 0 then -- spell is active\n            if not aura_env.up then\n                aura_env.up = true\n                --WeakAuras.regions[aura_env.id].region.icon:SetVertexColor(1,0,67/255)\n                --  if OOMM_WA_BORDER then \n                --    OOMM_WA_BORDER:BorderCreate(aura_env.id)\n                --  OOMM_WA_BORDER:BorderSetPoint(aura_env.id)\n                --end\n            end\n            --WILFRED\n            if WA_OOMM_WILFRED_DG then\n                local starttime = WA_OOMM_WILFRED_DG or 0\n                local activeend = starttime + duration\n                countdown = activeend - GetTime()\n                return string.format(\"%.1f\", countdown)\n            end\n            --\n            \n            return string.format(\"%.1f\", countdown - cooldown+duration)\n        end\n    else -- spell is available\n        if aura_env.cd then \n            -- PlaySoundFile(\"Interface\\\\AddOns\\\\OOMMsfx\\\\oomm-fx1-cd.ogg\",\"MASTER\")\n            aura_env.cd = false\n            aura_env.up = false\n            WeakAuras.regions[aura_env.id].region.icon:SetDesaturated(false)\n            --WeakAuras.regions[aura_env.id].region.icon:SetVertexColor(1,1,1)\n            \n        end\n        WeakAuras.regions[aura_env.id].region.stacks:SetVertexColor(1,1,1,1) --missing de color\n        --  WeakAuras.regions[aura_env.id].region.icon:SetBlendMode(\"BLEND\")\n        --WeakAuras.regions[aura_env.id].region.icon:SetVertexColor(1,1,1)\n        --if OOMM_WA_BORDER then \n        --  OOMM_WA_BORDER:BorderCreate(aura_env.id)\n        --    OOMM_WA_BORDER:BorderSetPoint(aura_env.id)\n        -- end\n        \n        return p or \"\"\n    end\nend",
