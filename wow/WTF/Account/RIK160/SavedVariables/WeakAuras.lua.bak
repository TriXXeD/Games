ar:SetHeight(aura_env[demon].data.height)\n        bar:SetWidth(aura_env[demon].data.width)\n        bar:SetPoint(\"TOPLEFT\",WeakAuras.regions[prefix..aura_env[demon].name].region,\"TOPLEFT\",0,0)\n        bar:SetFrameStrata(\"MEDIUM\")\n        \n        bar:SetStatusBarTexture(\"Interface/Buttons/WHITE8X8\")\n        bar.texture=bar:GetStatusBarTexture()\n        bar:SetBackdrop({bgFile = \"Interface/Tooltips/UI-Tooltip-Background\"})\n        bar:SetBackdropColor(0,0,0,0)\n        bar.duration = duration\n        bar:SetMinMaxValues(0, durationMax)\n        local spark = bar:CreateTexture(nil, \"ARTWORK\")\n        spark:SetDrawLayer(\"ARTWORK\", 3)\n        bar.spark = spark\n        bar.spark:SetTexture(aura_env[demon].data.sparkTexture)\n        bar.spark:SetWidth(aura_env[demon].data.sparkWidth)\n        bar.spark:SetHeight(aura_env[demon].data.sparkHeight)\n        bar.spark:SetPoint(\"CENTER\", bar, \"LEFT\", aura_env[demon].data.width, 0)\n        bar.spark:SetBlendMode(aura_env[demon].data.sparkBlendMode)\n        if count == 1 then\n            local bg = _G[barName]:CreateTexture(nil, \"LOW\", nil, -8)\n            bar.bg = bg\n            bar.bg:SetTexture(\"Interface/Buttons/WHITE8X8\")\n            local bgc = aura_env[demon].data.backgroundColor\n            bar.bg:SetVertexColor(bgc[1],bgc[2],bgc[3],bgc[4])\n            bar.bg:SetHeight(aura_env[demon].data.height)\n            bar.bg:SetWidth(aura_env[demon].data.width)\n            bar.bg:SetPoint(\"CENTER\", bar, \"CENTER\", 0, 0)\n        end\n    end\n    local bar = _G[barName]\n    -- these need to be set again on each Imp spawn\n    bar:SetFrameLevel(count>1000 and 0 or 1000-count)\n    bar.level = bar:GetFrameLevel()\n    bar.deUp = false\n    bar.expires = GetTime()+duration\n    bar:SetValue(duration)\n    local bc = aura_env[demon].data.barColor\n    aura_env.SetGradient(bar,bc,\"build----\")\n    bar.spark:SetPoint(\"CENTER\", bar, \"LEFT\", aura_env[demon].data.width, 0)\n    local sparkColor = aura_env[demon].data.sparkColor\n    bar.spark:SetVertexColor(sparkColor[1], sparkColor[2], sparkColor[3], sparkColor[4])\n    bar:Show()\nend\naura_env.SetGradient = function(bar,bc,...)\n    --print(string.format(\"\\124cFF%02x%02x%02x\", bc[1]*255, bc[2]*255, bc[3]*255),bar.name,...)\n    bar.texture:SetGradientAlpha(\"HORIZONTAL\",bc[1]*0.9,bc[2]*0.9,bc[3]*0.9,0.9,bc[1]*1.2,bc[2]*1.2,bc[3]*1.2,1)\nend\n\naura_env.DeIcons = function(demon,deUp)\n    local id = prefix..aura_env[demon].name\n    if WeakAuras.regions[id] and WeakAuras.regions[id].region and WeakAuras.regions[id].region.icon then \n        if deUp then\n            WeakAuras.regions[id].region.stacks:SetVertexColor(0,1,0,1) --de up color\n            -- WeakAuras.regions[id].region.icon:SetBlendMode(\"ADD\")\n            WeakAuras.regions[id].region.icon:SetVertexColor(1,1,1,1)\n        else \n            if UnitExists(\"pet\") and not UnitAura(\"pet\",GetSpellInfo(193396)) then\n                WeakAuras.regions[id].region.stacks:SetVertexColor(1,0,0,1) --missing de color\n                --WeakAuras.regions[id].region.icon:SetBlendMode(\"BLEND\")\n                WeakAuras.regions[id].region.icon:SetVertexColor(1,1,1,1)\n            end\n        end\n    end\nend\n\naura_env.DeBars = function(demon,deUp)\n    local id = prefix..aura_env[demon].name\n    if WeakAuras.regions[id] and WeakAuras.regions[id].region and WeakAuras.regions[id].region.bar then\n        local count = aura_env[demon].count\n        if deUp then\n            local bc = aura_env.de.data.barColor\n            for i=1,count,1 do\n                aura_env.SetGradient(_G[aura_env[demon].shortname..\"Bar\"..i],bc,i,\"deUp\",deUp)\n                _G[aura_env[demon].shortname..\"Bar\"..i].deUp = true\n            end\n        else\n            if UnitExists(\"pet\") and not UnitAura(\"pet\",GetSpellInfo(193396)) then\n                local bc = aura_env[demon].data.barColor\n                for i=1,count,1 do\n           