        local color=nil\n                    -- Find the identical expiration time\n                    for i=1,#(aura_env.spellblade) do\n                        if math.abs(aura_env.spellblade[i][2] - expires) < 1 then \n                            color=aura_env.spellblade[i][1]\n                            break\n                        end\n                    end\n                    -- If no identical expiration time found, add to the table\n                    if color==nil then\n                        for i=1,#(aura_env.spellblade) do\n                            if GetTime()>aura_env.spellblade[i][2] then \n                                aura_env.spellblade[i][2]=expires\n                                color=aura_env.spellblade[i][1]\n                                break\n                            end\n                        end\n                    end\n                    if color==nil then color = aura_env.GRAY end\n                    local _,_,_,frostbittenStacks=UnitDebuff(unit,GetSpellInfo(212647))\n                    aura_env.addTextureToNameplate(unit, circle, aura_env.L, color, aura_env.ALPHA_M,frostbittenStacks)\n                end\n                \n                -- Searing Brand\n                spell = GetSpellInfo(213166)\n                if UnitDebuff(unit, spell) then\n                    local role = UnitGroupRolesAssigned(unit)\n                    if role ~= \"TANK\" then\n                        if getRange(unit) >= 13 then\n                            aura_env.addTextureToNameplate(unit, circle, aura_env.L, aura_env.YELLOW, aura_env.ALPHA_M)\n                        else\n                            aura_env.addTextureToNameplate(unit, circle, aura_env.L, aura_env.RED, aura_env.ALPHA_M)\n                        end\n                    end\n                end\n                \n            elseif WeakAuras.CurrentEncounter and WeakAuras.CurrentEncounter.id==1862 then\n                --Tichondrius\n                \n      