						["custom_hide"] = "timed",
						["custom"] = "function(event, unit)\n    \n    --if event == \"UNIT_AURA\" or event == \"UNIT_SPELL_HASTE\" and unit ~= \"player\" then return true end\n    \n    local e = aura_env\n    e.stacks = 0\n    e.duration = 0\n    e.expTime = 0\n    \n    if event == \"PLAYER_SPECIALIZATION_CHANGED\" or event == \"UNIT_SPELL_HASTE\" then e.Recaliber() end\n    \n    if e.spell == 30108 then\n        for i = 1, 40 do\n            local dur, expTime, _, _, _, spellID = select(5, UnitDebuff(\"target\", i, \"PLAYER\"))\n            \n            if e.UnstableArray[spellID] then\n                e.stacks = e.stacks + 1\n                \n                if expTime > e.expTime then\n                    e.duration = dur\n                    e.expTime = expTime\n                end\n            end\n            \n            if not spellID then break end\n        end\n        \n    elseif e.spell == 264173 then\n        for i = 1, 40 do\n            local _, _, stacks, _, dur, expTime, _, _, _, spellID = UnitBuff(\"player\", i, \"PLAYER\")\n            \n            if spellID == e.spell then\n                e.stacks = stacks\n                e.duration = dur\n                e.expTime = expTime\n                \n                break\n            end\n            \n            if not spellID then break end\n        end\n        \n    else\n        local start, duration = GetSpellCooldown(e.spell)\n        \n        e.duration = duration > 1.5 and duration\n        e.expTime = duration > 1.5 and start + duration\n    end\n    \n    return true\nend",
