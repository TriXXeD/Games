						["subeventPrefix"] = "SPELL",
						["spellIds"] = {
						},
						["custom"] = "-- Some original Code in Action-tab made by Pottm@Serenity from his Star Augur weakaura\n-- Rewritten by (EU)Relodr-Blackrock - Reloe#2787 to make it work for other abilities and include some more features.\n-- Re-rewritten by (US)Bosmutus - Zul'jin\n\nfunction ()\n    -- Limit scan rate to improve performance\n    if GetTime()-(aura_env.LastScan or 0) < 0.3 then return end \n    aura_env.LastScan=GetTime()\n    \n    local unit = \"\"\n    local spell = \"\"\n    local spell2 = \"\"\n    local player = UnitName(\"player\")\n    local distance = -1\n    local duration = nil\n    local width = 60\n    local r = 1\n    local g = 0\n    local b = 0\n    local a = 0.5\n    local circle = \"Interface\\\\AddOns\\\\WeakAuras\\\\Media\\\\Textures\\\\Circle_White.tga\"\n    local shield = \"Interface\\\\Addons\\\\WeakAuras\\\\PowerAurasMedia\\\\Auras\\\\Aura21.tga\"\n    local handicap = \"Interface\\\\PVPFrame\\\\PVP-Banner-Emblem-1.tga\"\n    local jesus = \"Interface\\\\Addons\\\\WeakAuras\\\\PowerAurasMedia\\\\Auras\\\\Aura113.tga\"\n    \n    -- Only look up range when needed, and cache the value. \n    local getRange=function(unit)\n        if distance == -1 then distance = aura_env.rangeCheck(unit) end\n        return distance\n    end\n    \n    aura_env.everyFrame()\n    \n    if aura_env.debug then\n        --aura_env.addTextureToNameplate(\"target\", shield, 60, 0.75, 0.75, 0.75, 0.9, -1)\n        --aura_env.addTextureToNameplate(\"target\", handicap, aura_env.M, {1, 1, 1}, 1, nil, nil, 1)\n        --aura_env.addTextureToNameplate(\"target\", circle, aura_env.L, {0, 1, 1}, a, 7)\n        aura_env.addTextureToNameplate(\"target\", jesus, aura_env.S, {0.27,0.17,0.125}, 1, nil, nil, 1)\n        aura_env.addTextureToNameplate(\"target\", circle, aura_env.L, aura_env.WHITE, aura_env.ALPHA_M)\n    end\n    \n    for i=1, GetNumGroupMembers() do\n        distance = -1\n        unit = GetRaidRosterInfo(i)\n        if UnitExists(unit) then\n            unit = UnitName(unit)\n            \n            if WeakAuras.CurrentEncounter and WeakAuras.CurrentEncounter.id==2008 then\n                -- Helya\n                \n                -- Taint of the Sea\n                spell=GetSpellInfo(228054) \n                if UnitDebuff(unit,spell) then\n                    aura_env.addTextureToNameplate(unit, circle, aura_env.L, aura_env.GREEN_LS, aura_env.ALPHA_M)\n                end\n                -- Orb of Corrosion\n                spell=GetSpellInfo(230267) \n                if UnitDebuff(unit,spell) then\n                    aura_env.addTextureToNameplate(unit, circle, aura_env.L, aura_env.PURPLE_LS, aura_env.ALPHA_M)\n                end\n                -- Orb of Corruption\n                spell=GetSpellInfo(229119) \n                if UnitDebuff(unit,spell) then\n                    aura_env.addTextureToNameplate(unit, circle, aura_env.L, aura_env.PURPLE_LS, aura_env.ALPHA_M)\n                end\n                -- Fetid Rot\n                spell=GetSpellInfo(193367) \n                if UnitDebuff(unit,spell) then\n                    aura_env.addTextureToNameplate(unit, circle, aura_env.L, aura_env.GRAY, aura_env.ALPHA_M)\n                end\n                \n            elseif WeakAuras.CurrentEncounter and WeakAuras.CurrentEncounter.id==1962 then\n                --Guarm\n                \n                -- Flaming Volatile Foam\n                spell = GetSpellInfo(228744)\n                if UnitDebuff(unit, spell) then\n                    aura_env.addTextureToNameplate(unit, circle, aura_env.L, aura_env.RED, aura_env.ALPHA_M) \n                end\n                \n                -- Briney Volatile Foam\n                spell = GetSpellInfo(228810)\n                if UnitDebuff(unit, spell) then\n                    aura_env.addTextureToNameplate(unit, circle, aura_env.L, {0, 1, 0.7}, aura_env.ALPHA_M) \n                end\n                \n                -- Shadowy Volatile Foam\n        