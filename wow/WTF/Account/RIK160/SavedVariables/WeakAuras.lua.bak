		["Agony - Pandemic v3"] = {
			["sparkWidth"] = 1,
			["glow"] = false,
			["text1FontSize"] = 18,
			["xOffset"] = 0,
			["stacksFlags"] = "None",
			["yOffset"] = -222.5,
			["anchorPoint"] = "CENTER",
			["borderColor"] = {
				1, -- [1]
