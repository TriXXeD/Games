			["customText"] = "function()\n    if aura_env.state and aura_env.state.unit and aura_env.state.offset then\n        local region = aura_env.region\n        local plate = C_NamePlate.GetNamePlateForUnit(aura_env.state.unit)\n        if plate then \n            region:ClearAllPoints()\n            region:SetPoint(\"BOTTOM\", plate, \"TOPLEFT\", aura_env.state.offset, 0)\n            region:Show()\n        else\n            region:Hide()\n        end\n    end\nend",
