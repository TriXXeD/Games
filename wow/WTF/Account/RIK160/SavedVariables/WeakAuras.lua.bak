				["custom"] = "--Roraing Blaze Trigger\nfunction(event, ...)\n    \n    if event == \"ENCOUNTER_START\" or event == \"ENCOUNTER_END\" then -- if raid encounter starts, reset the table.\n        aura_env.immolateTable = {}\n        return true\n    end\n    \n    \n    local sourceGUID = select(4, ...) -- this only works for combat log event unfiltered, but its the only one we're importing anyway so yolo\n    \n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" and sourceGUID == UnitGUID('player') then \n        \n        --static variables-- (Could be put into OnInit, but for readability can be set here)\n        local SAR = \"SPELL_AURA_REMOVED\"\n        local SAA = \"SPELL_AURA_APPLIED\"\n        local SARe = \"SPELL_AURA_REFRESH\"\n        local SD= \"SPELL_DAMAGE\"\n        local ImmolateCastID = 348\n        local ImmolateDoTID = 157736\n        local ConflagID = 17962\n        ---------------------\n        \n        --event variables--\n        local destGUID = select(8, ...)\n        local subevent = select(2, ...)\n        local spellid = select(12, ...)\n        ----------------------\n        \n        if spellid == ImmolateDoTID and (subevent == SAA or subevent == SARe) then  \n            -- Immolate was applied or refreshed, add it to the table of dots and set the conflag notification to false\n            aura_env.immolateTable[destGUID] = 0\n            \n        elseif spellid == ImmolateDoTID and  subevent == SAR then \n            -- If Immolate was removed\n            aura_env.immolateTable[destGUID] = nil\n            \n        elseif spellid == ConflagID and subevent == SD then\n            -- Conflag was cast, check if the unit is in the table\n            if aura_env.immolateTable[destGUID] == nil then\n                return true -- do NOTHING because the target does not have immolate (maybe error check here with UnitDebuff if bugs occur?)\n                \n            else\n                aura_env.immolateTable[destGUID] = aura_env.immolateTable[destGUID] + 1\n                \n            end -- end table check\n        end-- end immolate/conflag check\n    end-- end event/sourceguid check\n    \n    return true\n    \nend",
